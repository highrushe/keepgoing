<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.prac.keepgoing.MemberMapper">
<insert id="insertMemeber">
insert into member(user_id,user_pw,user_email) 
values(#{user_id}, #{user_pw}, #{user_email})
</insert>

<select id="selectMember">
	select * from member;
</select>
</mapper>
